{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "regions": ["fra1"],
  "functions": {
    "api/**/*.py": { "maxDuration": 20, "memory": 1024 }
  },
  "rewrites": [
    { "source": "/InsightSuite/api/InsightChat", "destination": "/api/InsightChat" },
    { "source": "/InsightSuite/api/InsightChat/(.*)", "destination": "/api/InsightChat/$1" },
    { "source": "/InsightSuite/api/(.*)", "destination": "/api/insightsuite/$1" }
  ]
}
